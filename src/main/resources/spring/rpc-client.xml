        <property name="serialize" value="hessian" />
    </bean>
    <bean id="demoServiceRpc3" class="code.library.spring.factorybean.ClientProxy" init-method="init">
        <property name="serviceKey" value="DemoService3_Key" />
        <property name="interfaceName" value="code.library.netty4.DemoService" />
        <property name="serialize" value="hessian" />
    </bean>

</beans>